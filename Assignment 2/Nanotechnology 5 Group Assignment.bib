number = {12},
pages = {3931--3946},
pmid = {23571110},
publisher = {Elsevier Ltd},
title = {{Applications of nanotechnology in water and wastewater treatment}},
url = {http://dx.doi.org/10.1016/j.watres.2012.09.058},
volume = {47},
year = {2013}
