isbn = {0022-202X (Print)$\backslash$n0022-202X (Linking)},
issn = {0022202X},
journal = {J. Invest. Dermatol.},
keywords = {Acne Vulgaris,Acne Vulgaris: radiotherapy,Adult,Aged,Dose-Response Relationship, Radiation,Erythema,Erythema: etiology,Female,Humans,Male,Middle Aged,Skin,Skin Pigmentation,Skin Pigmentation: radiation effects,Skin: radiation effects,Spectrophotometry, Ultraviolet,Ultraviolet Rays,Ultraviolet Rays: adverse effects,Ultraviolet Therapy,Ultraviolet Therapy: instrumentation,Vitiligo,Vitiligo: radiotherapy},
number = {1},
pages = {42--47},
pmid = {6450810},
title = {{A New Apparatus with High Radiation Energy Between 320–460 nm: Physical Description and Dermatological Applications}},
