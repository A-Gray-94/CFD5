isbn = {1530-6992 (Electronic)$\backslash$n1530-6984 (Linking)},
issn = {15306984},
journal = {Nano Lett.},
keywords = {Desalination,grapheme,materials,molecular dynamics,nanofluidics},
number = {7},
pages = {3602--3608},
pmid = {22668008},
primaryClass = {dx.doi.org},
title = {{Water desalination across nanoporous graphene}},
