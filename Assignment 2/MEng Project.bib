file = {:C$\backslash$:/Users/Ali/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Feraboli, Norris, Mclarty - 2007 - Design and certification of a composite thin-walled structure for energy absorption.pdf:pdf},
issn = {0143-3369},
journal = {Int. J. Veh. Des. J. Veh. Des.},
keywords = {carbon fibre,composites,crashworthiness,destructive testing,energy absorption,impact,racecar},
number = {3},
pages = {247--267},
title = {{Design and certification of a composite thin-walled structure for energy absorption}},
volume = {4444},
year = {2007}
}
