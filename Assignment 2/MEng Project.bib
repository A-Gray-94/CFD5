pages = {1--8},
title = {{Crash Simulation of an F1 Racing Car Front Impact Structure}},
url = {http://www.heimbs-online.de/Heimbs{\_}2009{\_}Formula1Crash.pdf},
year = {2009}
}
@techreport{BigHead2015,
